{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "singularName": "vendor",
    "pluralName": "vendors",
    "displayName": "vendor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": false,
      "minLength": 8
    },
    "username": {
      "type": "string"
    },
    "organisation": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Approved",
        "Rejected",
        "Email Sent",
        "Activated"
      ]
    },
    "access_controls": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::access-control.access-control",
      "mappedBy": "vendor_id"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "vendor"
    },
    "refresh_token": {
      "type": "text"
    }
  }
}
