{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Approved",
        "Rejected"
      ]
    },
    "api_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::api-log.api-log",
      "mappedBy": "project_id"
    },
    "tokens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::token.token",
      "mappedBy": "project_id"
    },
    "project_apis": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-api.project-api",
      "mappedBy": "project_id"
    },
    "vendor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor",
      "inversedBy": "projects"
    }
  }
}
