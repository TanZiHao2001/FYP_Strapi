{
  "kind": "collectionType",
  "collectionName": "api_collections",
  "info": {
    "singularName": "api-collection",
    "pluralName": "api-collections",
    "displayName": "api_collection"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "api_collection_name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "string"
    },
    "access_controls": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::access-control.access-control",
      "mappedBy": "api_collection_id"
    },
    "api_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::api-category.api-category",
      "mappedBy": "api_collection_id"
    },
    "project_apis": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-api.project-api",
      "mappedBy": "api_collection_id"
    }
  }
}
