{
  "kind": "collectionType",
  "collectionName": "apis",
  "info": {
    "singularName": "api",
    "pluralName": "apis",
    "displayName": "api"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "method": {
      "type": "enumeration",
      "enum": [
        "GET",
        "POST",
        "DELETE",
        "PUT"
      ],
      "default": "GET",
      "required": true
    },
    "request_payload": {
      "type": "text"
    },
    "response_parameter": {
      "type": "text",
      "required": true
    },
    "api_log": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::api-log.api-log",
      "mappedBy": "api_id"
    }
  }
}
